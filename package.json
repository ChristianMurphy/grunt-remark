{
  "name": "grunt-remark",
  "version": "7.0.0",
  "description": "Grunt task for remark",
  "license": "MIT",
  "keywords": [
    "unified",
    "remark",
    "mdast",
    "markdown",
    "md",
    "grunt",
    "gruntplugin"
  ],
  "repository": "remarkjs/grunt-remark",
  "bugs": "https://github.com/remarkjs/grunt-remark/issues",
  "author": "Christian Murphy <Christian.Murphy.42@gmail.com> (https://github.com/ChristianMurphy)",
  "contributors": [
    "Christian Murphy <Christian.Murphy.42@gmail.com> (https://github.com/ChristianMurphy)",
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "files": [
    "tasks"
  ],
  "dependencies": {
    "remark": "^10.0.0",
    "unified-engine": "^6.0.0"
  },
  "peerDependencies": {
    "grunt": ">= 0.4.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/prompt-cli": "^8.0.0",
    "eslint": "^5.0.0",
    "eslint-config-google": "^0.13.0",
    "grunt": "^1.0.0",
    "grunt-cli": "^1.0.0",
    "husky": "^2.0.0",
    "remark-cli": "^6.0.0",
    "remark-preset-lint-consistent": "^2.0.0",
    "remark-preset-lint-recommended": "^3.0.0"
  },
  "scripts": {
    "test": "grunt && eslint . && remark . -qfo",
    "precommit": "npm test",
    "commitmsg": "commitlint -e",
    "commit": "commit"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "no-invalid-this": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-lint-recommended",
      "preset-lint-consistent"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "renovate": {
    "extends": [
      "schedule:weekly",
      "group:all",
      ":rebaseStalePrs",
      ":preserveSemverRanges",
      ":semanticPrefixChore",
      ":ignoreUnstable",
      ":renovatePrefix"
    ]
  }
}
