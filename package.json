{
  "name": "grunt-remark",
  "version": "3.0.0",
  "description": "Processes markdown with remark",
  "scripts": {
    "test": "grunt && eslint . && remark *.md -q --no-stdout"
  },
  "files": [
    "tasks"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChristianMurphy/grunt-remark.git"
  },
  "keywords": [
    "gruntplugin",
    "remark",
    "markdown"
  ],
  "author": {
    "name": "Christian Murphy",
    "email": "Christian.Murphy.42@gmail.com",
    "url": "https://github.com/ChristianMurphy"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChristianMurphy/grunt-remark/issues"
  },
  "homepage": "https://github.com/ChristianMurphy/grunt-remark#readme",
  "dependencies": {
    "remark": "^8.0.0",
    "unified-engine": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "~4.3.0",
    "eslint-config-google": "^0.9.1",
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "remark-cli": "^4.0.0",
    "remark-preset-lint-consistent": "^2.0.0",
    "remark-preset-lint-recommended": "^2.0.0"
  },
  "peerDependencies": {
    "grunt": ">= 0.4.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": "google",
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "no-invalid-this": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-lint-recommended",
      "preset-lint-consistent"
    ]
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
