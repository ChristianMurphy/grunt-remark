{
  "name": "grunt-remark",
  "version": "6.0.0",
  "description": "Processes markdown with remark",
  "scripts": {
    "test": "grunt && eslint . && remark *.md -q --no-stdout",
    "precommit": "npm test",
    "commitmsg": "commitlint -e",
    "commit": "commit"
  },
  "files": [
    "tasks"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remarkjs/grunt-remark.git"
  },
  "keywords": [
    "gruntplugin",
    "remark",
    "markdown"
  ],
  "author": {
    "name": "Christian Murphy",
    "email": "Christian.Murphy.42@gmail.com",
    "url": "https://github.com/ChristianMurphy"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/remarkjs/grunt-remark/issues"
  },
  "homepage": "https://github.com/remarkjs/grunt-remark#readme",
  "dependencies": {
    "remark": "^10.0.0",
    "unified-engine": "^6.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.0",
    "@commitlint/prompt-cli": "^7.0.0",
    "eslint": "^5.0.0",
    "eslint-config-google": "^0.11.0",
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "husky": "^1.0.0",
    "remark-cli": "^6.0.0",
    "remark-preset-lint-consistent": "^2.0.0",
    "remark-preset-lint-recommended": "^3.0.0"
  },
  "peerDependencies": {
    "grunt": ">= 0.4.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "no-invalid-this": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-lint-recommended",
      "preset-lint-consistent"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "renovate": {
    "extends": [
      "schedule:weekly",
      "group:all",
      ":rebaseStalePrs",
      ":preserveSemverRanges",
      ":semanticPrefixChore",
      ":ignoreUnstable",
      ":renovatePrefix"
    ]
  }
}
